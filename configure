	;;

      *-darwin* )
	STRIP_CR=True
	DEFAULT_FIXED_FONT="Monospace 7.5"
	;;
      # Otherwise we'll fall through to the generic opsys code at the bottom.
    esac
  ;;

  *) echo "Unknown system: ${host}"
     exit;;
esac

