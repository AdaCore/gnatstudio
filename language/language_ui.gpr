with "gtkada";
with "language";

project Language_UI is

   for Languages use ("Ada");
   for Source_Dirs use ("ui/src");
   for Object_Dir use "ui/obj";

   package Compiler is
      for Default_Switches ("Ada")
        use Language.Compiler'Default_Switches ("Ada");
   end Compiler;

   package IDE renames Language.IDE;

end Language_UI;
