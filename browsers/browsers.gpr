with "../gtkada/gtkada";
with "../prj_editor/prj_editor";
with "../shared";

project Browsers is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";

   package Compiler is
      for Default_Switches ("Ada") use ("-g", "-gnata", "-O2",
        "-gnaty", "-gnatwue");
   end Compiler;

   package IDE renames Shared.IDE;

end Browsers;
