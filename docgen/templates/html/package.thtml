@@--
@@-- Used to ouput the package declaration or ending
@@--
@@-- INDENT : number of spaces to be used for the indentation
@@-- NAME   : name of the package, used to reference it
@@-- BLOCK  : the package declaration or ending
@@--
<A NAME="@_NAME_@"></A><BR>
<TABLE BGCOLOR="WHITE" WIDTH="1%" CELLPADDING="0" CELLSPACING="0">
<TR><TD><PRE>
@_REPEAT(" "):INDENT_@
</PRE></TD>
<TD bgcolor="#DDDDDD">
<PRE>
@_BLOCK_@
</PRE></TD></TR></TABLE>
