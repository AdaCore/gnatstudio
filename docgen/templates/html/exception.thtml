@@--
@@-- Used for an exception declaration
@@--
@@-- INDENT    : number of spaces to be used for the indentation
@@-- DECL_LINE : declaration line for this exception
@@-- BLOCK     : the exception declaration
@@--
<A NAME="@_DECL_LINE_@"></A><BR>
<TABLE BGCOLOR="WHITE" WIDTH="1%" CELLPADDING="0" CELLSPACING="0">
<TR><TD><PRE>
@_REPEAT(" "):INDENT_@
</PRE></TD>
<TD bgcolor="#DDDDDD">
<PRE>
@_BLOCK_@
</PRE></TD></TR></TABLE>
