@SET_MAKE@

CFLAGS = -g -O2
ALL_CFLAGS = $(CFLAGS) @GTK_CFLAGS@ -I../gnat
LINK_FLAGS = ../gnat/gexpect.o ../pixmaps/pixmaps.o @GTK_LIBS@

MAIN = odd_main
EXEC = odd

all: $(EXEC)

$(EXEC): force
	gnatmake -m $(ALL_CFLAGS) $(MAIN) -o $(EXEC) -bargs -E \
	  -largs $(LINK_FLAGS)

clean:
	rm -f *.o *.ali *~ b~*

force:
