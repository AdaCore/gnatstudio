project GNAT_GVD is

   for Source_Dirs use ("gnat");
   for Object_Dir use "gnat";

   package Builder is
      for Default_Switches ("Ada") use ("-a");
   end Builder;

   package Compiler is
      for Default_Switches ("Ada") use ("-gnata", "-gnatpg", "-g", "-O2");
   end Compiler;

end GNAT_GVD;
