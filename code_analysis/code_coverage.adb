with Ada.Text_IO; use Ada.Text_IO;

package body Code_Coverage is

   -------------------
   -- Dump_Corevage --
   -------------------

   procedure Dump_Coverage (C_A : Coverage_Access) is
   begin
      Put (Integer'Image (C_A.Covered_Child_Count)
           & " /"
           & Integer'Image (C_A.Total_Child_Count));
   end Dump_Coverage;

end Code_Coverage;
