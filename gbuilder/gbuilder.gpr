with "../gtkada/gtkada";

project GBuilder is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";

   package Compiler is
      for Default_Switches ("Ada") use ("-g", "-O2", "-gnaty", "-gnatwue");
