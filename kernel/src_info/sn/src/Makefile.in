CC       = @CC@
SNAV = ../snsrc
INCLUDES = -I../include \
	   -I$(SNAV)/snavigator/hyper \
	   -I$(SNAV)/snavigator/parsers/toolbox \
	   -I$(SNAV)/db/include
LIBS = -L$(SNAV)/db/PORT

CFLAGS=

all : ../lib/db_capi.o

../lib/db_capi.o : db_capi.c
	$(CC) $(INCLUDES) ${CFLAGS} -g -c $< -o $@

clean :
	$(RM) ../lib/db_capi.o

distclean : clean
	$(RM) Makefile

.PHONY: all clean distclean 

