CC       = @CC@

INCLUDES = -I../include
LIBS = -L../lib

DEVALL=
DEVCLEAN=

-include Makefile.dev

.PHONY: all clean distclean

all : ../lib/db_capi.o $(DEVALL)

../lib/db_capi.o : db_capi.c
	$(CC) $(INCLUDES) -c $< -o $@

clean :
	$(RM) ../lib/db_capi.o $(DEVCLEAN)

distclean : clean
	$(RM) Makefile

