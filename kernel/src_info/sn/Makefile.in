all : snsrc src

snsrc :
	$(MAKE) -C snsrc all install

src :
	$(MAKE) -C src

clean :
	$(MAKE) -C src clean
	$(MAKE) -C snsrc clean

distclean : uninstall
	$(RM) Makefile config.cache include/config.h config.log config.status
	$(MAKE) -C src distclean
	$(MAKE) -C snsrc distclean

uninstall :
	rm -f include/cdefs.h include/db.h include/mpool.h \
		include/snptools.h include/sn.h \
		bin/*browser bin/db* lib/lib*

install :

.PHONY: snsrc src
