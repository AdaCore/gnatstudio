<?xml version='1.0'?>
<GPS>
  <key load='include/common'/>

  <key action="Toggle to next alias field">primary-Tab</key>
  <key action="Expand alias under cursor">primary-o</key>

  <key action="Macro Stop">primary-Escape</key>
  <key action="Repeat Next">primary-y</key>

  <!--  Do not use gtk+ builtin handling for cut/copy/paste, since that
        preserves the tags. For instance, copy-pasting non-editable text would
        keep the text as read-only. -->
  <key action="Cut to Clipboard">primary-x</key>
  <key action="Cut to Clipboard">shift-Delete</key>
  <key action="Copy to Clipboard">primary-c</key>
  <key action="Copy to Clipboard">primary-Insert</key>
  <key action="Paste From Clipboard">primary-v</key>
  <key action="Paste From Clipboard">shift-Insert</key>
  <key action="Paste Previous From Clipboard">shift-primary-Insert</key>
  <key action="select all">primary-a</key>

  <key action="debug display local variables">primary-l</key>
  <key action="debug display arguments">primary-u</key>
  <key action="debug interrupt">primary-backslash</key>
  <key action="continue till line">primary-b</key>

  <key action="No casing/indentation on next key">primary-q</key>
  <key action="Toggle auto casing/indentation">alt-q</key>
  <key action="save">primary-s</key>
  <key action="Close current window">primary-w</key>
  <key action="undo">primary-z</key>
  <key action="redo">primary-r</key>
  <key action="comment lines">primary-slash</key>
  <key action="uncomment lines">primary-question</key>
  <key action="refill">primary-equal</key>
  <key action="goto line">primary-g</key>
  <key action="jump to matching delimiter">primary-apostrophe</key>

  <key action="Delete word backward" >control-BackSpace</key>
  <key action="Delete word forward" >control-Delete</key>

  <key action="Search">primary-f</key>
  <key action="Replace" >shift-primary-F</key>
  <key action="find next">primary-n</key>
  <key action="find previous">primary-p</key>

  <key action="Remove message">alt-Delete</key>

  <key action="Global Search" >primary-u</key>
  <key action="Global Search in context: Entities">primary-t</key>

  <key action="exit">primary-q</key>
  <key action="backward locations history">Button_4</key>
  <key action="forward locations history">Button_5</key>

</GPS>
