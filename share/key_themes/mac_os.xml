<?xml version='1.0'?>
<!--  This file contains shortcuts that are specific to Mac OS.-->
<GPS>
  <key load='default'/>

  <key action="goto beginning of line">control-a</key>
  <key action="goto beginning of line">primary-Left</key>
  <key action="goto end of line">control-e</key>
  <key action="goto end of line">primary-Right</key>
  <key action="kill line">control-k</key>
  <key action="Delete word backward" >alt-BackSpace</key>
  <key action="Delete word forward" >alt-Delete</key>

</GPS>
