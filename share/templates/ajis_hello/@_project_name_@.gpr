project @_Project_Name_@ is

   for Languages use ("Ada", "Java");
   for Source_Dirs use ("src_ada", "src_java/**");  
   for Object_Dir use "obj";

   package Compiler is
      for Switches ("ada") use ("-gnat05");
   end Compiler;

   package Make is
      for Makefile use "Makefile";
   end Make;

end @_Project_Name_@;

