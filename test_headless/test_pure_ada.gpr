with "vss_text";

project Test_Pure_Ada is

   for Languages use ("Ada");
   for Source_Dirs use ("src");
   for Main use ("test_pure_ada.adb");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";

   package Builder is
      for Executable ("test_pure_ada.adb") use "test_pure_ada";
   end Builder;

   package Compiler is
      for Switches ("Ada") use ("-g", "-gnat2022", "-gnatwa", "-O0");
   end Compiler;

end Test_Pure_Ada;
